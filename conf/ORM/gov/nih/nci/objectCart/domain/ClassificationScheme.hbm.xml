<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="gov.nih.nci.objectCart.domain">
	<class name="ClassificationScheme" table="CLASSIFICATION_SCHEME" lazy="true" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="integer" column="ID">
			<generator class="assigned" />
		</id>
		<!-- Attributes mapping for the ClassificationScheme class -->
		<property name="labelTypeFlag" type="string" column="LABEL_TYPE_FLAG"/>
		<property name="type" type="string" column="TYPE"/>
		<!-- Associations mapping for the ClassificationScheme class -->
	</class>
</hibernate-mapping>