<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="gov.nih.nci.objectCart.domain">
	<class name="Cart" table="CART" lazy="false" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="integer" column="ID">
			<generator class="identity" />
		</id>
		<!-- Attributes mapping for the Cart class -->
		<property name="creationTime" type="java.util.Date" column="CREATION_TIME"/>
		<property name="expirationDate" type="java.util.Date" column="EXPIRATION_DATE"/>
		<property name="lastActive" type="java.util.Date" column="LAST_ACTIVE"/>
		<property name="name" type="string" column="NAME"/>
		<property name="userId" type="string" column="USER_ID"/>
		<!-- Associations mapping for the Cart class -->
		<set name="cartObjectCollection" lazy="false" cascade="all,delete-orphan">
			<cache usage="read-write" />
			<key column="CART_ID" not-null="true" />
			<one-to-many class="gov.nih.nci.objectCart.domain.CartObject"/>
		</set>
		<many-to-one name="classificationScheme" class="gov.nih.nci.objectCart.domain.ClassificationScheme" column="CLASSIFICATION_SCHEME_ID" lazy="false" />
	
		<filter name="expirationDate"
            		condition=":current_date() &lt;= EXPIRATION_DATE"/>
	</class>
</hibernate-mapping>